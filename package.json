{
  "name": "heroku-cra-node",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "6.11.x"
  },
  "scripts": {
    "start": "set NODE_ENV=production && node server",
    "dev": "nodemon server",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.3",
    "cool-ascii-faces": "1.3.4",
    "express": "^4.16.3",
    "express-jsonschema": "^1.1.0",
    "googleapis": "^22.2.0",
    "mime": "^2.3.1",
    "mongo-express": "^0.49.0",
    "nodemon": "^1.18.2",
    "passport": "^0.4.0",
    "passport-spotify": "^0.3.1",
    "react": "^15.6.2",
    "react-audio-player": "^0.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-player": "^0.20.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-sound": "^0.8.0",
    "react-youtube": "7.5.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "request": "^2.87.0",
    "spotify-web-api-node": "^3.1.1",
    "ws": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
